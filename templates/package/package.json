{
  "name": "@pixeloven/{name}",
  "version": "{version}",
  "description": "{description}",
  "homepage": "https://github.com/pixeloven/pixeloven",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixeloven/pixeloven.git"
  },
  "author": "{author}",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@pixeloven/cli": "0.5.0"
  },
  "scripts": {
    "prepare": "yarn lint && yarn test && yarn compile",
    "clean": "pixeloven-cli clean",
    "compile": "pixeloven-cli compile",
    "precompile": "pixeloven-cli compile:clean",
    "lint": "pixeloven-cli lint",
    "test": "pixeloven-cli test",
    "test:watch": "pixeloven-cli test:watch",
    "pretty": "pixeloven-cli pretty",
    "pretest": "pixeloven-cli test:clean"
  }
}
